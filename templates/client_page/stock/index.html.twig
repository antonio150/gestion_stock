{% extends 'client_page/index.html.twig' %}

{% block title %}Stock
{% endblock %}

{% block body %}
{# {{require}} #}
	<div>
		<a class="btn btn-primary" href="{{path('app_stock')}}">Retour stock</a>
	</div>

	
	<table class="table">
		<thead>
			<tr>

				<th scope="col">Produit</th>
				<th scope="col">Quantite</th>
			</tr>
		</thead>
		<tbody>
		
			{% for items in listeStock %}
				<tr>

					<td>{{items[0]}}</td>
					<td>{{items[1]}}</td>

					

				</tr>
			{% endfor %}
		</tbody>
	</table>
	

{% endblock %}
